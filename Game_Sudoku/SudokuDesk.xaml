                    <GradientStop Color="#80000000" Offset="0.498"/>
                    <GradientStop Color="#80FFFFFF" Offset="1"/>
                    <GradientStop Color="#80F9F9F9"/>
                </LinearGradientBrush>
            </Rectangle.Stroke>
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                    <GradientStop Color="#A5001485" Offset="0.056"/>
                    <GradientStop Color="#FF113200" Offset="0.064"/>
                    <GradientStop Color="#FF09350B" Offset="0.534"/>
                    <GradientStop Color="#FF113100" Offset="0.072"/>
                    <GradientStop Color="#FF248B00" Offset="0.96"/>
                    <GradientStop Color="#A50E3500" Offset="1"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
